(function () {
  var names = ["Yaakov", "John", "Jen", "Jason", "Paul", "Frank", "Larry", "Paula", "Laura", "Jim"];

  // Creating 'helloSpeaker' object
  var helloSpeaker = {};
  helloSpeaker.speak = function (name) {
    console.log("Hello " + name);
  };

  // Creating 'byeSpeaker' object
  var byeSpeaker = {};
  byeSpeaker.speak = function (name) {
    console.log("Goodbye " + name);
  };

  // Looping through the array
  for (var i = 0; i < names.length; i++) {
    var firstLetter = names[i].charAt(0).toLowerCase(); // Get first letter (case-insensitive)

    if (firstLetter === 'j') {
      byeSpeaker.speak(names[i]); // Print "Goodbye" for names starting with 'J'
    } else {
      helloSpeaker.speak(names[i]); // Print "Hello" for other names
    }
  }
})();
